        <pose>0 0 0 0 0 0</pose>
        <camera>
          <image>
            <width>640</width>
            <height>480</height>
            <format>R8G8B8</format>
          </image>
          <clip>
            <near>0.05</near>
            <far>100</far>
          </clip>
          <lens>
            <type>custom</type>
            <vfov>1.5147</vfov>
            <c1>1.0</c1>
            <f>1.0</f>  
          </lens>
        </camera>

        <always_on>true</always_on>
        <update_rate>30</update_rate>
        <visualize>true</visualize>
        <topic>camera</topic>

        <plugin name="gz::sim::systems::CameraVideoRecorder"
          filename="ignition-gazebo-camera-video-recorder-system" />
      </sensor>
    </link>

    <joint name="camera_joint" type="fixed">
